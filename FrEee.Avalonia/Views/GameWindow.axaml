<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:FrEee.Avalonia.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="FrEee.Avalonia.Views.GameWindow"
        Title="FrEee">
	<DockPanel Name="dockPanel">
		<TextBlock DockPanel.Dock="Left" Text="left" HorizontalAlignment="Center" VerticalAlignment="Center" Background="Blue"/>
		<TextBlock DockPanel.Dock="Right" Text="right" HorizontalAlignment="Center" VerticalAlignment="Center" Background="Red"/>
		<TextBlock DockPanel.Dock="Top" Text="top" HorizontalAlignment="Center" VerticalAlignment="Center"/>
		<TextBlock DockPanel.Dock="Bottom" Text="bottom" HorizontalAlignment="Center" VerticalAlignment="Center"/>
		<TextBlock Text="CENTER" HorizontalAlignment="Center" VerticalAlignment="Center"/>
	</DockPanel>
</Window>
