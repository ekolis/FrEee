<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net472</TargetFramework>
    <LangVersion>7.3</LangVersion>
    <ApplicationIcon>FrEee.ico</ApplicationIcon>
    <OutputType>WinExe</OutputType>
    <StartupObject />
    <Version>0.0.9</Version>
    <Authors>FrEee Dev Team (see licenses/credits.txt)</Authors>
    <Company />
    <Product>FrEee</Product>
    <AssemblyName>FrEee</AssemblyName>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <OutputPath></OutputPath>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <OutputPath></OutputPath>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="Forms\MogreCombatForm.cs" />
    <Compile Remove="Forms\MogreCombatForm.Designer.cs" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Remove="Forms\MogreCombatForm.resx" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="NAudio" Version="1.8.4" />
    <PackageReference Include="NAudio.Vorbis" Version="1.0.0" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\FrEee\FrEee.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Windows.Forms" />
  </ItemGroup>
  <ItemGroup>
    <None Update="Assets\Splash.jpg">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>
  <Target Name="PostBuild" AfterTargets="PostBuildEvent">
    <Exec Command="&quot;$(ProjectDir)post-build.bat&quot; $(ConfigurationName) $(ProjectDir)" />
  </Target>
 </Project>