<Window x:Class="FrEee.Wpf.Views.ShellView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:regions="http://www.codeplex.com/CompositeWPF"
        xmlns:wpf="clr-namespace:FrEee.Wpf"
        Title="{Binding Title}"
        Width="1250"
        Height="800"
        MinWidth="900"
        MinHeight="600"
        SnapsToDevicePixels="True"
        UseLayoutRounding="True">
    <Grid>
        <ContentControl Focusable="False" IsTabStop="False" regions:RegionManager.RegionName="{x:Static wpf:RegionNames.MainRegion}" />
    </Grid>
</Window>
